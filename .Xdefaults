URxvt*termName: rxvt-unicode-256color

URxvt.intensityStyles: false
URxvt.background:       [90]#000000
URxvt.depth:  32

URxvt.font:\
    xft:FuraCode Nerd Font:style=Regular:size=13,\
    xft:Fira Code Medium:style=Regular:size=13,\
    xft:DejaVu Sans Mono:style=Book:pixelsize=13,\
    xft:DejaVuSansMonoForPowerline\ Nerd\ Font:style=Book:pixelsize=13,\
    xft:Inconsolata\ for\ Powerline:pixelsize=13,\
    xft:EmojiOne,\
    xft:Symbola,\
    xft:Noto Emoji:minspace=False

URxvt.boldFont:\
    xft:FuraCode Nerd Font:style=Bold:size=13,\
    xft:Fira Code Medium:style=Bold:size=13,\
    xft:DejaVu Sans Mono:style=bold:pixelsize=13,\
    xft:DejaVuSansMonoForPowerline Nerd Font:style=Bold:pixelsize=13

URxvt.italicFont:\
    xft:FuraCode Nerd Font:style=Oblique:size=13,\
    xft:DejaVu Sans Mono:style=Oblique:pixelsize=13,\
    xft:DejaVuSansMonoForPowerline Nerd Font:style=Oblique:pixelsize=13


URxvt*boldItalicFont:\
    xft:FuraCode Nerd Font:style=BoldOblique:size=13,\
    xft:DejaVu Sans Mono:style=BoldOblique:pixelsize=13,\
    xft:DejaVuSansMonoForPowerline Nerd Font:style=Bold Oblique:pixelsize=13

URxvt.letterSpace:          -1

URxvt.scrollBar: false
URxvt.cursorColor: white
URxvt.cursorColor2: #696969

!! Extensions
URxvt.perl-ext-common: default,matcher,resize-font,url-select,keyboard-select
URxvt.colorUL:  #4682B4
!! url-select
URxvt.keysym.M-u: perl:url-select:select_next
URxvt.url-select.launcher: /usr/bin/xdg-open
URxvt.url-select.underline: true
URxvt.url-launcher:
!! Prevent url from opening twice when using mouse
URxvt.url-select.button: 2

!! Ctrl+arrow in console
URxvt.keysym.Control-Up:     \033[1;5A
URxvt.keysym.Control-Down:   \033[1;5B
URxvt.keysym.Control-Left:   \033[1;5D
URxvt.keysym.Control-Right:  \033[1;5C

URxvt.keysym.Shift-Control-V: eval:paste_clipboard
URxvt.keysym.Shift-Control-C: eval:selection_to_clipboard

!! keyboard-select:
URxvt.keysym.M-Escape: perl:keyboard-select:activate

!! Matcher
URxvt.url-launcher: /usr/bin/xdg-open
URxvt.matcher.button: 1

URxvt.iso14755: False
URxvt.iso14755_52: False

!! for tmux
URxvt.keysym.C-S-Left: string:\033[1;6D
URxvt.keysym.C-S-Right: string:\033[1;6C

rofi.color-enabled: true
rofi.color-window: #000, #000, #000
rofi.color-normal: #111, #819396, #222, #008ed4, #ffffff
rofi.color-active: #002b37, #008ed4, #003643, #008ed4, #66c6ff
rofi.color-urgent: #002b37, #da4281, #003643, #008ed4, #890661

rofi.fake-transparency: true
rofi.lines:  10
rofi.bw:  0
rofi.opacity: "10"
rofi.hide-scrollbar: true
rofi.width:  30
