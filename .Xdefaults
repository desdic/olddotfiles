URxvt.scrollBar: false
URxvt.cursorColor: white
URxvt.cursorColor2: #696969

!! Extensions
URxvt.perl-ext-common: default,matcher,resize-font,url-select,keyboard-select
URxvt.colorUL:  #4682B4
!! url-select
URxvt.keysym.M-u: perl:url-select:select_next
URxvt.url-select.launcher: /usr/bin/xdg-open
URxvt.url-select.underline: true
URxvt.url-launcher:
!! Prevent url from opening twice when using mouse
URxvt.url-select.button: 2

!! Ctrl+arrow in console
URxvt.keysym.Control-Up:     \033[1;5A
URxvt.keysym.Control-Down:   \033[1;5B
URxvt.keysym.Control-Left:   \033[1;5D
URxvt.keysym.Control-Right:  \033[1;5C

URxvt.keysym.Shift-Control-V: eval:paste_clipboard
URxvt.keysym.Shift-Control-C: eval:selection_to_clipboard

!! keyboard-select:
URxvt.keysym.M-Escape: perl:keyboard-select:activate

!! Matcher
URxvt.url-launcher: /usr/bin/xdg-open
URxvt.matcher.button: 1

URxvt.iso14755: False
URxvt.iso14755_52: False

!! for tmux
URxvt.keysym.C-S-Left: string:\033[1;6D
URxvt.keysym.C-S-Right: string:\033[1;6C

